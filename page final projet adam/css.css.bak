    color: yellow;
    font-weight:normal;
}



.button {
    display: inline-block;
    text-align: center;
    vertical-align: middle;
    padding: 12px 24px;
    border: 1px solid #09d0e6;
    border-radius: 8px;
    background: #00d9ff;
    background: -webkit-gradient(linear, left top, left bottom, from(#00d9ff), to(#d90000));
    background: -moz-linear-gradient(top, #00d9ff, #d90000);
    background: linear-gradient(to bottom, #00d9ff, #d90000);
    -webkit-box-shadow: #6600ff 0px 0px 40px 0px;
    -moz-box-shadow: #6600ff 0px 0px 40px 0px;
    box-shadow: #6600ff 0px 0px 40px 0px;
    text-shadow: #800000 1px 1px 1px;
    font: normal normal bold 20px tahoma;
    color: #ffffff;
    text-decoration: none;
}
.button:hover,
.button:focus {
    border: 1px solid #0aefff;
    background: #00ffff;
    background: -webkit-gradient(linear, left top, left bottom, from(#00ffff), to(#ff0000));
    background: -moz-linear-gradient(top, #00ffff, #ff0000);
    background: linear-gradient(to bottom, #00ffff, #ff0000);
    color: #ffffff;
    text-decoration: none;
}
.button:active {
    background: #008299;
    background: -webkit-gradient(linear, left top, left bottom, from(#008299), to(#d90000));
    background: -moz-linear-gradient(top, #008299, #d90000);
    background: linear-gradient(to bottom, #008299, #d90000);
}
.button:before{
    content:  "\0000a0";
    display: inline-block;
    height: 24px;
    width: 24px;
    line-height: 24px;
    margin: 0 4px -6px -4px;
    position: relative;
    top: 0px;
    left: 0px;
    background: url("data:image/png;base64,") no-repeat left center transparent;
    background-size: 100% 100%;
}

